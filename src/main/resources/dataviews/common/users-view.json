{
	"id": "users",
	"table": "c2_user",
	"displayName": "Usuario",
	"sql": "active = true",
	"fields": [
		{
			"id": "id"
		},
		{
			"id": "name"
		},
		{
			"id": "lastname"
		},
		{
			"id": "role"
		},
		{
			"id": "active",
			"type": "bool"
		},
		{
			"id": "homepage"
		},
		{
			"id": "attempts",
			"type": "number"
		},
		{
			"id": "nextAttempt",
			"type": "date"
		},
		{
			"id": "roleInfo",
			//from role where id = parent.role
			"parentReference": "role",
			"childReference": "id",
